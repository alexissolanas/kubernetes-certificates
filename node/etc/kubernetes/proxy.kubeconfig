apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/certs/ca.crt
    server: https://atomic0:6443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: proxy
  name: proxy-ctx
current-context: proxy-ctx
users: 
- name: proxy
  user:
    client-certificate: /etc/kubernetes/certs/atomic1.crt 
    client-key: /etc/kubernetes/certs/atomic1.key 
